{
  "name": "automart",
  "version": "1.0.0",
  "description": "AutoMart is an auto dealership/sales application",
  "main": "app.js",
  "scripts": {
    "test": "nyc --reporter=html --reporter=text mocha src/tests/*.test.js --require @babel/register --exit",
    "start:dev": "nodemon --exec babel-node src/app.js",
    "prebuild": "rm -rf build && mkdir build",
    "build:babel": "babel --out-dir build --ignore *.test.js src",
    "build": "npm run prebuild && npm run build:babel",
    "start": "npm run build && node ./build/app.js",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "coveralls": "nyc --reporter=lcov --reporter=text-lcov npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ebenezermakinde/AutoMart.git"
  },
  "keywords": [
    "cars",
    "bikes",
    "vans",
    "coupes",
    "buy",
    "sell",
    "automart",
    "bus"
  ],
  "author": "Ebenezer Makinde",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ebenezermakinde/AutoMart/issues"
  },
  "homepage": "https://github.com/ebenezermakinde/AutoMart#readme",
  "dependencies": {
    "@babel/polyfill": "^7.4.4",
    "app-root-path": "^2.2.1",
    "bcrypt": "^3.0.6",
    "body-parser": "^1.19.0",
    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.11",
    "morgan": "^1.9.1",
    "validatorjs": "^3.15.1",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/node": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "codeclimate-test-reporter": "^0.5.1",
    "coveralls": "^3.0.3",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.17.3",
    "istanbul": "^0.4.5",
    "mocha": "^6.1.4",
    "mocha-lcov-reporter": "^1.3.0",
    "nodemon": "^1.19.1",
    "nyc": "^14.1.1"
  }
}
